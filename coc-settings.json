{
  "codeLens.enable": true,
  "diagnostic.errorSign": "☠",
  "diagnostic.hintSign": "☀",
  "diagnostic.infoSign": "➜",
  "diagnostic.messageTarget": "echo",
  "diagnostic.virtualText": true,
  "diagnostic.warningSign": "⚡",
  "eslint.alwaysShowStatus": true,
  "solargraph.bundlerPath": "~/.rbenv/shims/bundle",
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "solargraph.useBundler": false,
  "solargraph.completion": true,
  "solargraph.transport": "socket",
  "solargraph.checkGemVersion": true,
  "solargraph.hover": true,
  "suggest.noselect": false,
  "tsserver.enableJavascript": true,
  "javascript.referencesCodeLens.enable": false,
  "yaml.completion": true,
  "yaml.schemaStore.enable": true,
  "yaml.validate": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.width": 30,
  "explorer.buffer.root.template": "[icon & 1] OPEN EDITORS",
  "explorer.file.root.template": "[icon & 1] [hidden & 1] Project: ([root]) [fullpath]",
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]",
  "explorer.previewAction.onHover": false,
  "Lua.diagnostics.globals": [
    "vim"
  ],
  "languageserver": {
    "R": {
      "command": "/usr/bin/R",
      "args" : [ "--slave", "-e", "languageserver::run()"],
      "filetypes" : ["r"]
    }
  },
  "Lua.workspace.library" :{
    "/usr/share/nvim/runtime/lua": true,
    "/usr/share/nvim/runtime/lua/vim/lsp": true
  },
  "cSpell.userWords": [
    "FHIR",
    "Marp",
    "Nerdfont",
    "Onedocway",
    "Todos",
    "UNITEDHEALTHCARE",
    "WALKIN",
    "alexesba",
    "automock",
    "currenlty",
    "expl",
    "fullpath",
    "glyphicon",
    "javascripts",
    "multiline",
    "noselect",
    "nrun",
    "nvim",
    "optum",
    "ordinalized",
    "pylama",
    "resouce",
    "serializers",
    "solargraph",
    "webm"
  ]
}
